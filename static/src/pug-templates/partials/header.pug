header(role="banner" aria-label="Site header")
    button.toggle-highcontrast(title="High contrast mode",data-activetext="high contrast on") high contrast off
    nav(role="navigation" aria-label="Site navigation")
        ul
            each val, index  in sitenav
                li
                    a(href=val.url)= val.title
                    if(val.subnav)
                        ul
                            each subval, index in val.subnav
                                li: a(href=subval.url)= subval.title
